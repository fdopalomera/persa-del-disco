erdiagram Store

notation=default

entity Product {
   id key
}

entity Order {
   id key
}

entity OrderItem {
   id key
}

entity Customer {
   id key
}

relationship has {
   Order[1] -> OrderItem[N]
}

relationship are_in {
   Product[N] -> OrderItem[N]
}

relationship makes {
   Customer[1] -> Order[N]
}